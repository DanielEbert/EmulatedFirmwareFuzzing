void fuzz_test(char* buf, size_t buf_len) {
  DynamicJsonDocument doc(256);
  DeserializationError ret = deserializeJson(doc, buf, buf_len);

  if (ret) {
    Serial.print("deserializeJson() failed: ");
    Serial.println(ret.c_str());
    Serial.flush();
  }
  else {
    Serial.println("deserializeJson() succeeded");
  }
  delay(1200);
}

